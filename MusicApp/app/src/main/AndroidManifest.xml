<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MyPlaylistActivity"
            android:label="@string/category_playlist" />
        <activity
            android:name=".SearchActivity"
            android:label="@string/category_search" />
        <activity
            android:name=".MyLibraryActivity"
            android:label="My Library" />
        <activity
            android:name=".PopMusicActivity"
            android:label="Pop Music" />
        <activity
            android:name=".IndianMusicActivity"
            android:label="Indian Music" />
        <activity
            android:name=".WorkoutMusicActivity"
            android:label="Workout Music" />
        <activity
            android:name=".MoviesMusicActivity"
            android:label="TV and Movies" />

    </application>

</manifest>